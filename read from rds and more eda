cur = conn.cursor()

SQL_Query_1 = pd.read_sql('SELECT * FROM teatetotal ORDER BY teatetotal.pedido OFFSET 0 ROWS FETCH NEXT 100000 ROWS ONLY', connDB)
SQL_Query_2 = pd.read_sql('SELECT * FROM teatetotal ORDER BY teatetotal.pedido OFFSET 100001 ROWS FETCH NEXT 100000 ROWS ONLY', connDB)
SQL_Query_3 = pd.read_sql('SELECT * FROM teatetotal ORDER BY teatetotal.pedido OFFSET 200001 ROWS FETCH NEXT 100000 ROWS ONLY', connDB)
SQL_Query_4 = pd.read_sql('SELECT * FROM teatetotal ORDER BY teatetotal.pedido OFFSET 300001 ROWS FETCH NEXT 100000 ROWS ONLY', connDB)
SQL_Query_5 = pd.read_sql('SELECT * FROM teatetotal ORDER BY teatetotal.pedido OFFSET 400001 ROWS FETCH NEXT 100000 ROWS ONLY', connDB)
SQL_Query_6 = pd.read_sql('SELECT * FROM teatetotal ORDER BY teatetotal.pedido OFFSET 500001 ROWS FETCH NEXT 180000 ROWS ONLY', connDB)
SQL_Query_7 = pd.read_sql('SELECT * FROM teatetotal ORDER BY teatetotal.pedido OFFSET 500001 ROWS FETCH NEXT 600000 ROWS ONLY', connDB)

SQL_complete_1=pd.concat([SQL_Query_1,SQL_Query_2,SQL_Query_3,SQL_Query_4,SQL_Query_5,SQL_Query_6])
SQL_complete_1.shape

SQL_Query=SQL_complete_1.copy()
SQL_Query['valortotalpedido']=SQL_Query.valortotalpedido.astype(float)
SQL_Query['valoringresofactur']=SQL_Query.valoringresofactur.astype(float)
SQL_Query['valoringresopedido']=SQL_Query.valoringresopedido.astype(float)
SQL_Query['valorunitariopedido']=SQL_Query.valorunitariopedido.astype(float)
SQL_Query['cantidaddepedido']=SQL_Query.cantidaddepedido.astype(float)
SQL_Query['valorcuponteate']=SQL_Query['valorcuponteate'].replace('','0')

SQL_Query['valorcuponteate']=SQL_Query.valorcuponteate.astype(float)

SQL_Query['valorcupondescuento']=SQL_Query['valorcupondescuento'].replace('','0')
SQL_Query['descuentoteate']=SQL_Query['descuentoteate'].replace('','0')

SQL_Query['valorcupondescuento']=SQL_Query.valorcupondescuento.astype(float)

QL_Query['descuentoteate']=SQL_Query.descuentoteate.astype(float)


