SQL_Query['valortotcondscto']=SQL_Query.valortotcondscto.astype(float)

SQL_Query['month_year'] = pd.to_datetime(SQL_Query['fechapedido']).dt.to_period('M')

SQL_Query['nombrecat'].value_counts()

bebidas=SQL_Query[SQL_Query['nombrecat']=='BEBIDAS']

confiteria=SQL_Query[SQL_Query['nombrecat']=='CONFITERIA']

mascotas=SQL_Query[SQL_Query['nombrecat']=='MASCOTAS']

alimentos=SQL_Query[SQL_Query['nombrecat']=='ALIMENTOS']

aseo=SQL_Query[(SQL_Query['nombrecat']=='ASEO HOGAR') | (SQL_Query['nombrecat']=='DETERGENTE PARA ROPA')]

cuidado=SQL_Query[SQL_Query['nombrecat']=='ASEO PERSONAL']
medicamentos=SQL_Query[SQL_Query['nombrecat']=='MEDICAMENTOS']
otras=SQL_Query[SQL_Query['nombrecat']=='OTRAS CATEGORIAS']


aseo['nombrecat']=aseo['nombrecat'].replace('DETERGENTE PARA ROPA', 'ASEO HOGAR')
aseo.nombrecat.unique()

