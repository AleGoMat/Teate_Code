otras['otras']=pd.get_dummies(otras['nombrecat'])
aseo['aseo']=pd.get_dummies(aseo['nombrecat'])
alimentos['alimentos']=pd.get_dummies(alimentos['nombrecat'])
mascotas['mascotas']=pd.get_dummies(mascotas['nombrecat'])
bebidas=bebidas[['pedido','bebidas']]
confiteria=confiteria[['pedido','confiteria']]
medicamentos=medicamentos[['pedido','medicamentos']]
otras=otras[['pedido','otras']]
aseo=aseo[['pedido','aseo']]
mascotas=mascotas[['pedido','mascotas']]
alimentos=alimentos[['pedido','alimentos']]
